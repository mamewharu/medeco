<div class="side-header">
  <div class="header-name">
   <%= link_to "#{current_user.name}さん", edit_user_path(current_user)%>
   <h6>ようこそ!</h6>
  </div>
  <div class="create-room">
   <%= link_to "ルーム作成はこちら", new_room_path %>
  </div>
</div>

<div class="side-rooms">
<% current_user.rooms.each do |room|%>
 <div class=room>
   <div class=room-name>
     <%= link_to room.name, room_messages_path(room) %>
   </div>
 </div>
 <% end %>
</div>